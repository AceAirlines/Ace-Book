<html style="overflow: hidden">
  <head>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;400;600;800&display=swap" rel="stylesheet">
    <style>
      @keyframes extend {
        0% {
          width: 350px;
        }
        100% {
          width: 80%;
        }
      }

      @keyframes extendContent {
        0% {
          opacity: 0;
        }
        100% {
          opacity: 1;
        }
      }
      ::-webkit-scrollbar{
        width: 10px;
        background-color: white;
      }

      ::-webkit-scrollbar-thumb{
        width: 10px;
        border-radius: 5px;
        background-color: #c2c2c2;
      }
      button:hover {
        background-color: #cf0c0c;
        margin-top: 50px;
        width: 26%; 
        height: 43px;
        transition: all .5s;
      }
      button {
        background-color: #e60f0f;
        margin-top: 50px;
        width: 25%; 
        height: 40px;
        transition: all .5s;
      }
    </style>
    <style type="text/css">
        @media print { body { -webkit-print-color-adjust: exact; } }
    </style>
  </head>
  <body style="margin: 0px;">

     <!--Header-->   
     
     <div style="width: 100vw; height: 80px; background-color: white; border-bottom: 1.5px solid #ECECEC;">
      <img src="https://flyaceairline.weebly.com/uploads/1/2/4/2/124276951/published/ace-logo-new.png?1594390182" style="height: 40px; margin: 20px">
    </div>

    <!--Boarding Pass-->

    <div id="print-iframe" style="margin-top: 75px; background-color: transparent;">
      <center>
        <img src="scissors.png" style="float: left; margin-left: 10%; width: 13px "> </img>
        <div id="containerPass" style="border: 1px dashed black; background-color: #f3f3f3; width: 75%; height: auto">
          <img src="https://flyaceairline.weebly.com/uploads/1/2/4/2/124276951/ace-boarding-pass_orig.png" style="margin: 0px; width: 100%; float: center;" ></img>
      </center>
    </div>
    <center>
        <div id="Acecontent" style="z-index: 1000; display: flex; text-align: center; position: absolute; left: 12.5%; top: 155px; background-color: transparent; width: 75%;">
        <div style="margin-left: 25%; margin-top: 3%;">
        <p style="font-family: open sans; display: inline-block; font-size: 2vw; color: #e60f0f;">HSV &nbsp&nbsp ATL</p>
        <p style="font-family: open sans; margin-left: 13.5vw; font-size: 2vw; display: inline-block; color: black;">Ben Lambertson</p>
        <div>
        <p style="font-family: open sans; margin: 2vw; margin-left: -41vw; font-size: 1.5vw; display: inline-block; color: black;">Ben Lambertson</p>
        <p style="font-family: open sans; margin: 2vw; margin-left: 3vw; font-size: 1.5vw; display: inline-block; color: black;">January 5, 2021</p>
        </div>
        <div>
        <p style="font-family: open sans; margin: 2vw; margin-left: -10vw; margin-top: 1vw; font-size: 3vw; display: inline-block; color: black;">Huntsville</p>
        <p style="font-family: open sans; margin: 2vw; margin-left: 2vw; margin-top: 1vw; font-size: 3vw; display: inline-block; color: #e60f0f;">1:45</p>
        </div>
        <div>
        <p style="font-family: open sans; margin: 2vw; margin-left: -10vw; margin-top: 1vw; font-size: 3vw; display: inline-block; color: black;">HSV</p>
        </div>
        </div>
    </div>
    </center>

    <!--Verify-->
    
        <div id="info" style="display: block; z-index: 1000; position: absolute; top: 0px; left: 0px; background-color: white; width: 100%; height: 100vh">
          <div id="navigation" style="width: 100vw; height: 80px; background-color: white; border-bottom: 1.5px solid #ECECEC;">
      <img src="https://flyaceairline.weebly.com/uploads/1/2/4/2/124276951/published/ace-logo-new.png?1594390182" style="height: 40px; margin: 20px">
    </div>
    <center style="margin-top: 20px">
      <b style="color: #e60f0f; font-size: 25px; font-family: open sans;" >Flight Information</b>
      <i style="display: block; color: #adadad; font-size: 50px; font-family: open sans;" >HSV-ATL</i>

<div style="margin-top: 30px; ">
      <div style="width: 49.5%; display: table-cell;">
      <b style="color: #e60f0f; font-size: 25px; font-family: open sans;" >Time</b>
      <i style="display: block; color: #adadad; font-size: 25px; font-family: open sans;" >1:45 PM</i>
      </div>

      <div style="display: table-cell; height: 20%; background-color: #adadad;">
        </div>

      <div style="width: 49.5%; display: table-cell;">
      <b style="color: #e60f0f; font-size: 25px; font-family: open sans;" >Date</b>
      <i style="display: block; color: #adadad; font-size: 25px; font-family: open sans;" >January 5th, 2021</i>
      </div>
</div>

      <button id="print" onclick="printer()" style="font-family: open sans; font-size: 14px; border: none; border-radius: 5px; color: white;">Print Boarding Pass</button>
    </center>
      </div>
  </body>
  <script>    
    function button(){
      document.getElementById('print').style.display = 'block';
      document.getElementById('info').style.display = 'block';
    setTimeout(button, 500);
    }

    function printer() {
      document.getElementById('print').style.display = 'none'; 
      document.getElementById('info').style.display = 'none'; 
      window.print();
        document.getElementById('print').style.display = 'block'; 
      document.getElementById('info').style.display = 'block'; 
    }

    //Pull Data
    
    </script>
</html>